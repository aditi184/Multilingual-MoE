{
  "streams": [
    {
      "name": "commoncrawl-nsfw-highthresh-ablation",
      "documents": [
        "pretraining-data/sources/common-crawl/v1-small/documents/*.gz"
      ],
      "output": {
        "path": "pretraining-data/sources/common-crawl/v1-small-nsfw-filtered-highthresh/documents",
        "max_size_in_bytes": 3894967296
      },
      "attributes": [
        "nsfw_cc_final",
        "decontamination"
      ],
      "filter": {
        "include": [
          "$.attributes[?(@.nsfw_cc_final__whitespace_tokenizer_with_paragraphs_v1__document[0][2] < 100000)]"
        ],
        "exclude": [
          "$.attributes[?(@.nsfw_cc_final__whitespace_tokenizer_with_paragraphs_v1__document[0][2] < 50)]",
          "$@.attributes[?(@.bff_duplicate_paragraph_spans && @.bff_duplicate_paragraph_spans[0] && @.bff_duplicate_paragraph_spans[0][2] >= 1.0)]"
        ]
      },
      "span_replacement": [
        {
          "span": "$.attributes.nsfw_cc_final__jigsaw_nsfw_sencence_v2____label__nsfw",
          "min_score": 0.4,
          "replacement": ""
        }
      ]
    }
  ],
  "work_dir": {
    "input": "/tmp/mixer-nsfw-highthresh-v1/input",
    "output": "/tmp/mixer-nsfw-highthresh-v1/output"
  },
  "processes": 96
}
