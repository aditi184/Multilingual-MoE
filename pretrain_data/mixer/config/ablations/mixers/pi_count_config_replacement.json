{
        "streams": [
        {
                "name": "commoncrawl-pi-less-than-5-anonymize",
                "documents": [
                        "pretraining-data/sources/common-crawl/v1-small/documents/*.gz"
                ],
                "output": {
                        "path": "pretraining-data/sources/common-crawl/v1-small-pi-less-than-5-anonymize/documents",
                        "max_size_in_bytes": 3894967296
                },
                "attributes": [
                        "hatespeech_nsfw_cc_v3",
                        "decontamination",
                        "pi_regex_count_tagger"
                ],
                "filter": {
                        "include": [
                                "$.attributes[?(@.pi_regex_count_tagger__pii_regex_with_counts_v1__doc_count[0][2] < 5)]",
                                "$.attributes[?(@.hatespeech_nsfw_cc_v3__whitespace_tokenizer_with_paragraphs_v1__document[0][2] < 100000)]"
                        ],
                        "exclude": [
                                "$.attributes[?(@.hatespeech_nsfw_cc_v3__whitespace_tokenizer_with_paragraphs_v1__document[0][2] < 50)]",
				"$@.attributes[?(@.bff_duplicate_paragraph_spans && @.bff_duplicate_paragraph_spans[0] && @.bff_duplicate_paragraph_spans[0][2] >= 1.0)]"
                        ]
                },
                "span_replacement": [
                {
                        "span": "$.attributes.pi_regex_count_tagger__pii_regex_with_counts_v1__EMAIL_ADDRESS",
                        "min_score": 0.5,
                        "replacement": " ||EMAIL_ADDRESS|| "
                },
                {
                        "span": "$.attributes.pi_regex_count_tagger__pii_regex_with_counts_v1__PHONE_NUMBER",
                        "min_score": 0.5,
                        "replacement": " ||PHONE_NUMBER|| "
                },
                {
                        "span": "$.attributes.pi_regex_count_tagger__pii_regex_with_counts_v1__IP_ADDRESS",
                        "min_score": 0.5,
                        "replacement": " ||IP_ADDRESS|| "
                }
                ]
        }
        ],
        "work_dir": {
                "input": "/data2/pi_mix/tmp/mixer-pi-less-than-5-anonymize-v1/input",
                "output": "/data2/pi_mix/tmp/mixer-pi-less-than-5-anonymize-v1/output"
        },
        "processes": 90
}
