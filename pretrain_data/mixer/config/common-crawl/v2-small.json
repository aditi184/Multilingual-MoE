{
  "streams": [
    {
      "name": "v2_small",
      "documents": [
        "pretraining-data/sources/common-crawl/v1-small/documents/*"
      ],
      "output": {
        "path": "pretraining-data/sources/common-crawl/v2-small/documents",
        "max_size_in_bytes": 1
      },
      "attributes": [
        "dedupe_paragraphs"
      ],
      "span_replacement": [
        {
          "span": "$.attributes.bff_duplicate_paragraph_spans",
          "min_score": 0.5,
          "replacement": ""
        }
      ]
    }
  ],
  "work_dir": {
    "input": "/data1/work/input",
    "output": "/data2/work/output"
  },
  "processes": 128
}
