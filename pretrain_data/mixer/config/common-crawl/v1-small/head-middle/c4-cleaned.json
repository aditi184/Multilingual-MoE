{
  "streams": [
    {
      "name": "v1_small",
      "documents": [
        "pretraining-data/sources/common-crawl/v1-small-head-middle/documents/*"
      ],
      "output": {
        "path": "pretraining-data/sources/common-crawl/v1-small-head-middle-c4-cleaned/documents",
        "max_size_in_bytes": 1,
        "discard_fields": ["attributes", "metadata", "added", "created"]
      },
      "attributes": [
        "c4_rules",
        "decontamination"
      ],
      "filter": {
        "include": [],
        "exclude": [
          "$.attributes[?(@.bff_duplicate_paragraph_spans && @.bff_duplicate_paragraph_spans[0] && @.bff_duplicate_paragraph_spans[0][2] >= 1.0)]"
        ]
      },
      "span_replacement": [
        {
          "span": "$.attributes.c4_rules__c4_v1__lines_with_no_ending_punctuation",
          "min_score": 0.5,
          "replacement": ""
        },
        {
          "span": "$.attributes.c4_rules__c4_v1__lines_with_too_few_words",
          "min_score": 0.5,
          "replacement": ""
        }
      ]
    }
  ],
  "work_dir": {
    "input": "/data1/work/input",
    "output": "/data2/work/output"
  },
  "processes": 128
}
